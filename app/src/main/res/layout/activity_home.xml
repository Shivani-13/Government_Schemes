<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity"
 android:background="#6347"
>

    <ImageButton
        android:id="@+id/b1"
        android:layout_width="90sp"
        android:layout_height="90sp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="55dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/agricul"
        android:padding="20sp"
        android:paddingHorizontal="30sp"
        android:visibility="visible" />

    <ImageButton
        android:id="@+id/b2"
        android:layout_width="90sp"
        android:layout_height="90sp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/b1"
        android:layout_marginEnd="55dp"
        android:background="@drawable/rural"
        android:padding="20sp"
        android:paddingHorizontal="30sp"
        android:visibility="visible" />

    <ImageButton
        android:id="@+id/b3"
        android:layout_width="90sp"
        android:layout_height="90sp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/b1"
        android:layout_marginTop="185dp"
        android:background="@drawable/edu"
        android:padding="20sp"
        android:paddingHorizontal="30sp"
        android:visibility="visible" />

    <ImageButton
        android:id="@+id/b4"
        android:layout_width="90sp"
        android:layout_height="90sp"
        android:layout_alignStart="@+id/b2"
        android:layout_alignTop="@+id/b3"
        android:background="@drawable/health"
        android:padding="20sp"
        android:paddingHorizontal="30sp"
        android:visibility="visible" />

    <ImageButton
        android:id="@+id/b5"
        android:layout_width="90sp"
        android:layout_height="90sp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/b1"
        android:layout_marginBottom="100dp"
        android:background="@drawable/women"
        android:padding="20sp"
        android:paddingHorizontal="30sp"
        android:visibility="visible" />

    <ImageButton
        android:id="@+id/b6"
        android:layout_width="90sp"
        android:layout_height="90sp"
        android:layout_alignStart="@+id/b2"
        android:layout_alignTop="@+id/b5"
        android:background="@drawable/chid"
        android:padding="20sp"
        android:paddingHorizontal="30sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/t1"
        android:layout_width="90sp"
        android:layout_height="30sp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/b1"
        android:layout_marginTop="127dp"
        android:gravity="center"
        android:text="Agriculture"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="90sp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/b2"
        android:layout_alignTop="@+id/t1"
        android:gravity="center"
        android:text="Rural"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t3"
        android:layout_width="90sp"
        android:layout_height="30sp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/b1"
        android:layout_marginBottom="230dp"
        android:layout_below="@id/b3"
        android:gravity="center"
        android:text="Education"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t4"
        android:layout_width="90sp"
        android:layout_height="30sp"
        android:layout_alignBottom="@+id/t3"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/b2"
        android:layout_marginBottom="230dp"
        android:layout_below="@id/b4"
        android:gravity="center"
        android:text="Health"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t5"
        android:layout_width="90sp"
        android:layout_height="44dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/b1"
        android:layout_marginBottom="40dp"
        android:gravity="center"
        android:text="Women welfare"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/t6"
        android:layout_width="90sp"
        android:layout_height="44dp"
        android:layout_alignStart="@+id/b2"
        android:layout_alignTop="@+id/t5"
        android:gravity="center"
        android:text="Child welfare"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold" />

</RelativeLayout>